<form method="post" action="{{ path('app_contacto_delete', {'id': contacto.id}) }}" onsubmit="return confirm('¿Estás seguro de que quieres eliminar este elemento?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contacto.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
